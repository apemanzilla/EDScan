<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="200.0" minWidth="400.0" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView fx:id="table" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn editable="false" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" sortable="false" text="Enabled" />
            <TableColumn editable="false" minWidth="100.0" prefWidth="100.0" sortable="false" text="Plugin" />
            <TableColumn editable="false" maxWidth="10000.0" minWidth="250.0" prefWidth="250.0" sortable="false" text="Description" />
         </columns>
      </TableView>
   </center>
   <bottom>
      <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <children>
            <Label maxWidth="1.7976931348623157E308" text="EDScan must be restarted to apply changes." HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#openPluginFolder" text="Open Plugin Folder" />
         </children>
      </HBox>
   </bottom>
</fx:root>
